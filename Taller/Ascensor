import java.util.Random;

public class Ascensor {

  private int pisoActual;
  public final int PISO_MAXIMO;
  public final int PISO_MAXIMO;
}
    public Ascensor() {
        Random random = new Random();
        pisoActual = random.nextInt(7) + 1;
}

    public int getPisoActual() {
        return pisoActual;
}

  public Ascensor(int max, int min)
    PISO_MAXIMO=max
    PISO_MINIMO=min
    pisoActual=PISO_MINIMO
}

public void subirPiso(){
  if (pisoActual>=PISO_MINIMO)
    pisoActual++;
}

public void bajarPiso(){
  if (pisoActual<=PISO_MAXIMO)
    pisoActual--;
}

public int getPisoActual(){
  return pisoActual;
}

