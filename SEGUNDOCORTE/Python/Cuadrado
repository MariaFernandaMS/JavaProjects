class  Cuadrado:

    def __init__(self,lado):
        self.lado = lado
    
    def area(self):
        return self.lado * 2
    
    def perimetro(self):
        return self.lado * 4
    
cuadrado = Cuadrado(12)
print("Area del cuadrado: ", cuadrado.area())
print("Perimetro del cuadrado: ", cuadrado.perimetro())
